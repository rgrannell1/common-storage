{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "deno",
      "command": "test",
      "problemMatcher": [
        "$deno-test"
      ],
      "args": [
        "--unstable",
        "--allow-all"
      ],
      "group": "test",
      "label": "ðŸ¦¾ Deno Test"
    },
    {
      "label": "Acceptance Test Build",
      "dependsOrder": "sequence",
      "dependsOn": [
        "Remove Local Server",
        "Remove Remote Server"
      ]
    },
    {
      "label": "Remove Local Server",
      "type": "shell",
      "command": "rm /tmp/local-server || true",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Remove Remote Server",
      "type": "shell",
      "command": "rm /tmp/remote-server || true",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
